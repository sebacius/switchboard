# Switchboard UI Dashboard Configuration
# Copy to /etc/switchboard/ui.env

# HTTP server settings
UI_PORT=3000
UI_BIND=0.0.0.0

# Signaling server backends (comma-separated, name=url format)
# Example: server1=http://localhost:8080,server2=http://192.168.1.10:8080
UI_BACKENDS=local=http://localhost:8080

# Logging level: debug, info, warn, error
LOG_LEVEL=info
